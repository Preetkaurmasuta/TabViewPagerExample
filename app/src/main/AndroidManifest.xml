<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adesa.interview.tabviewpagerexample">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:name="com.moxdroid.interview.tabviewpagerexample.AppConfig"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="net.hockeyapp.android.appIdentifier"
            android:value="${HOCKEYAPP_APP_ID}" />

        <activity android:name="com.moxdroid.interview.tabviewpagerexample.slidepageview.MainActivity" />
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.tabpageview.HomeActivity" />
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.TimerTestActivity" />
        <activity
            android:name="com.moxdroid.interview.tabviewpagerexample.runtimepermission.RunTimePermissionsTestActivity"
            android:label="@string/title_activity_run_time_permission"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.moxdroid.interview.tabviewpagerexample.activitytofragment.MainFragmentActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.FloatingLableTestActivity" />
        <activity
            android:name="com.moxdroid.interview.tabviewpagerexample.VideoLiveStreamActivity"
            android:label="@string/title_activity_video_live_stream"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.volleytest.ImageLoadingActivity" />
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.PhoneStateListenerActivity" />
        <activity
            android:name="com.moxdroid.interview.tabviewpagerexample.ScreenOrientationTestActivity"
            android:configChanges="orientation|keyboardHidden|screenSize" />

        <service
            android:name="com.moxdroid.interview.tabviewpagerexample.downloads.DataDownloadIntentService"
            android:exported="false" />

        <activity android:name="com.moxdroid.interview.tabviewpagerexample.downloads.DataDownloadActivity" />
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.BaseActivity" />
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.databasegreendao.FetchLoaderActivity" />
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.exampleloopj.NetworkCallActivity" />
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.multitasking.HandlerExampleActivity" />
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.CustomLibraryTestActivity" />
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.multitasking.PeriodicHandlerActivity" />
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.CustomProgressBarActivity" />
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.firebaseexample.FireBaseExampleActivity" />

        <service
            android:name="com.moxdroid.interview.tabviewpagerexample.firebaseexample.MyFirebaseMessagingService"
            android:enabled="true"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service
            android:name="com.moxdroid.interview.tabviewpagerexample.firebaseexample.MyFirebaseInstanceIDService"
            android:enabled="true"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity android:name="com.moxdroid.interview.tabviewpagerexample.InputLayoutActivity">

        </activity>
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.BottomTabNavigationActivity" />
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.TirangleBadgeViewActivity" />
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.fragmentexample.TabOrMobileActivity"></activity>
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.chat.ChatActivity"></activity>
        <activity android:name="com.moxdroid.interview.tabviewpagerexample.LinearLayoutListActivity"></activity>

        <activity android:name="com.moxdroid.interview.tabviewpagerexample.HelloAndroid">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>